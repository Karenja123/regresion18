x=c(100,125,125,150,200,200,250,300,300,350,400,400)

m1<-data.frame(b0,b1,s)
m1
y1=m1[1,1]+m1[1,2]*x
y1
y2=m1[2,1]+m1[2,2]*x
y2
y3=m1[3,1]+m1[3,2]*x
y3
y4=m1[4,1]+m1[4,2]*x
y4
y5=m1[5,1]+m1[5,2]*x
y5
y6=m1[6,1]+m1[6,2]*x
y6
y7=m1[7,1]+m1[7,2]*x
y7
y8=m1[8,1]+m1[8,2]*x
y8
y9=m1[9,1]+m1[9,2]*x
y9
y10=m1[10,1]+m1[10,2]*x
y10
lines(x,y10)
