install.packages("ggplot2")
library("ggplot2")


b1<-c(1.7559,1.64,1.4699,1.6944,1.4497,1.7309,1.889,1.6471,1.7216,1.7058)
b0<-c(-60.62,-49.40,-4.8,-41.95,5.8,-70.01,-95.01,-40.3,-42.68,63.31)
